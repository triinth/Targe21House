@model targe21house.Models.House.HouseDeleteViewModel

<h1>Delete</h1>

<div>

    <h4>House</h4>
    <hr />
    <dl class="row">
        <dl class="row">
            <div class="form-group row">
                <label asp-for="Id" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Id)
            </div>

            <div class="form-group row">
                <label asp-for="Address" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Address)
            </div>

            <div class="form-group row">
                <label asp-for="RoomCount" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.RoomCount)
            </div>

            <div class="form-group row">
                <label asp-for="Size" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Size)
            </div>

            <div class="form-group row">
                <label asp-for="Price" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Price)
            </div>

            <div class="form-group row">
                <label asp-for="BuiltDate" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.BuiltDate)
            </div>

            <div class="form-group row">
                <label asp-for="Country" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Country)
            </div>

            <div class="form-group row">
                <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.CreatedAt)
            </div>

            <div class="form-group row">
                <label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.ModifiedAt)
            </div>

    </dl>
</div>
<hr />
<form asp-action="DeleteConfirmation">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="index" asp-controller="house" class="btn btn-success">Back</a>
</form>


